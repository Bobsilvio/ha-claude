{
  "name": "Claude AI Backend",
  "version": "1.0.0",
  "slug": "claude-backend",
  "description": "All-in-one add-on - Automatically deploys Claude component and runs backend API for Claude AI Assistant",
  "url": "https://github.com/Bobsilvio/ha-claude",
  "arch": [
    "aarch64",
    "amd64",
    "armv7"
  ],
  "startup": "application",
  "apparmor": false,
  "boot": "auto",
  "map": [
    "config:rw"
  ],
  "ports": {
    "5000/tcp": 5000
  },
  "ports_description": {
    "5000/tcp": "Claude Backend API"
  },
  "options": {
    "ha_url": "http://homeassistant:8123",
    "api_port": 5000,
    "debug_mode": false
  },
  "schema": {
    "ha_url": "str",
    "api_port": "int",
    "debug_mode": "bool"
  }
}
